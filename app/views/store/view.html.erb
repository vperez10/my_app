<h1 align="center"> View Stores </h1>


  <% @stores.each do |store| %>
  <div class="container">
    <div class="row">
        <div class="col-sm-offset-3 col-sm-2"> <%= store.name %> </div>
        <div class="col-sm-2"> <%= store.address %> </div>
        <div class="col-sm-2"> <%= store.type %> </div>
        <div class="col-sm-2"> <%= store.hours %> </div>
    </div>
</div>
  <% end %>

